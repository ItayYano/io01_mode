        "cmd": "sudo nmap -O -v"
    },

    "s01_07": 
    {
        "type": "run",
        "desc": "discover.T0888.001: OT SysInfo Discovery-enip--> enip_fingerprint <target_ip>", 
        "cmd": "sudo nmap -Pn -sU -p44818 --script enip-info"
    },
    
    "s01_08":
    {
        "type": "run",
