        "cmd": "sudo nmap -O -v"
    },

    "enip_fingerprint": 
    {
        "type": "run",
        "desc": "CIP/enip fingerprinting <target_device>", 
        "cmd": "sudo nmap -Pn -sU -p44818 --script enip-info"
    },
 
