   {
    "io01_ver": {
        "type": "run",
        "desc": "Show kadabra commands help", 
        "cmd": "echo Cloud 1.0.1"
    },

    "io01_update": {
        "type": "run",
        "desc": "Update  kadabra", 
        "cmd": "sh ./io01_update.sh"
    },

      "sublist3r": {
        "type": "run",
        "desc": "Use Sublist3r to find subdomains INPUT:[target_website]",
        "cmd": "sudo python3 ./third_party/Sublist3r/sublist3r.py -d"
    },

    "sysinfo": 
    {
        "type": "run",
        "desc": "discover.T0846.001: TCP SysInfo Discovery--> tcp_fingerprint <target_ip>", 
        "cmd": "sudo nmap -O -v"
    },
      
  "nmap-vulners": 
    {
        "type": "run",
        "desc": "Prints out known vulns <target_ip>", 
        "cmd": "   nmap --script nmap-vulners/ -sV "
    },
      
       "dirb": {
        "type": "run",
        "desc": "Scan the target website for hidden directories and files INPUT:[target_website]",
        "cmd": "sudo dirb"
    },
     
         "curl": {
        "type": "run",
        "desc": "curl <URL>",
        "cmd": "sudo curl"
    },
     
      "domain-info": {
        "type": "run",
        "desc": "translte domain to ip and get info from ipinfo.io",
        "cmd": "sudo sh ./third_party/unc1549/demo/domain_ip_info.sh unc1549_c2_domains.txt out_c2.json"
    },

     
  "dig": {
        "type": "run",
        "desc": "dns lookup using dig",
        "cmd": "sudo dig"
    },
  "createfile": {
        "type": "run",
        "desc": "create a floder BringThemeHomeNow with relvant files based on UNC1549",
        "cmd": "sudo sh ./third_party/unc1549/demo/create_files.sh UNC1549Files.txt"
    },
       "yara": {
        "type": "run",
        "desc": "run yara",
        "cmd": "sudo yara"
    },
         "trivy": {
        "type": "run",
        "desc": "trivy",
        "cmd": "sudo trivy"
    }  
}
